// Imported Stylesheets
@import 'bootstrap.min.css'
@import 'bootstrap-theme.min.css'
@import '../fonts/typography.css'
@import 'gradients.css'
@import 'customize.styl'
@import 'nib/vendor'

vendor-prefixes = webkit moz o ms official

// Color Variables
white = #ffffff
black = #0a0a16
gray = #7c7c7c
blue = #425da2
green = #83b744
orange = #f35c34

txtColor = #1b1f29
titleColor = #3e4452
subtitleColor = #425da2

lightGray = #dfdee3
lightBlue = #5b76bc
lightGreen = #a4cb74
lightOrange = #f79074

darkGray = #2c2c2c
darkBlue = #293a66
darkGreen = #608532
darkOrange = #d6370d

// Font Variables 
copyTxt = 'ralewayregular', Arial, Helvetica, sans-serif
title = 'ralewayheavy', Arial, Helvetica, sans-serif
subtitle = 'ralewaymedium', Arial, Helvetica, sans-serif
buttonsFont = 'ralewaybold', Arial, Helvetica, sans-serif
ligthFont = 'ralewaylight', Arial, Helvetica, sans-serif

openRegular = 'OpenSansRegular', Arial, Helvetica, sans-serif
openBold = 'OpenSansBold', Arial, Helvetica, sans-serif

// -------------- Theme Styles -------------- //

// General Styles
html
	font-size 16px
	max-width 100%

body
	background blue
	color txtColor
	font-family copyTxt
	font-size 62.5% /* 1em = 10px */
	position relative
	max-width 100%

.hidden-seo
	height 0
	left -9999px
	position absolute
	top -9999px
	width 0

a 
	transition all .2s ease-in-out

	&:hover
		color green

.title 
	color titleColor
	font-family title
	margin-top 0

.subtitle
	color subtitleColor
	font-family subtitle

.green
	color green

.orange
	color orange

.blue 
	color blue

.darkBlue 
	color darkBlue

.pick-ui-icon-sm
	background darkBlue url(../img/picks-ui-sprite_01.png) no-repeat
	border 1px solid rgba(255,255,255,0.2)
	border-radius 50%
	box-shadow 1px 1px 0px 0px rgba(0,0,0,0.3)
	display inline-block
	height 32px
	overflow hidden
	width 32px

.pick-ui-icon-sm.soccer-pick
	background-position -10px -9px

.pick-ui-icon-sm.box-pick
	background-position -60px -9px

.pick-ui-icon-sm.tennis-pick
	background-position -109px -9px

.pick-ui-icon-sm.basketball-pick
	background-position -159px -9px

.pick-ui-icon-sm.baseball-pick
	background-position -209px -9px

.pick-ui-icon-sm.hockey-pick
	background-position -258px -10px

.pick-ui-icon-sm.americano-pick
	background-position -309px -9px

.pick-ui-icon-md
	background url(../img/picks-ui-sprite_01.png) no-repeat
	display inline-block
	height 50px
	overflow hidden
	width 50px

.pick-ui-icon-md.soccer-pick
	background-position 0 -50px

.pick-ui-icon-md.box-pick
	background-position -50px -50px

.pick-ui-icon-md.tennis-pick
	background-position -100px -50px

.pick-ui-icon-md.basketball-pick
	background-position -150px -50px

.pick-ui-icon-md.baseball-pick
	background-position -200px -50px

.pick-ui-icon-md.hockey-pick
	background-position -250px -50px

.pick-ui-icon-md.americano-pick
	background-position -300px -50px

.pick-ui-icon-lg
	background #293A68 url(../img/picks-ui-sprite_01.png) no-repeat
	border 3px solid blue
	border-radius 50%
	box-shadow 1px 1px 3px 1px rgba(0,0,0,0.3)
	box-sizing border-box
	display inline-block
	height 64px
	overflow hidden
	width 64px

.pick-ui-icon-lg.soccer-pick
	background-position 4px -96px

.pick-ui-icon-lg.box-pick
	background-position -46px -96px

.pick-ui-icon-lg.tennis-pick
	background-position -96px -96px

.pick-ui-icon-lg.basketball-pick
	background-position -146px -98px

.pick-ui-icon-lg.baseball-pick
	background-position -196px -98px

.pick-ui-icon-lg.hockey-pick
	background-position -246px -96px

.pick-ui-icon-lg.americano-pick
	background-position -297px -96px

// Layout Styling
.container
	max-width 960px

